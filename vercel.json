{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    }
  ],
  "build": {
    "env": {
      "DATABASE_URL": "@my-database-url"
    },
    "commands": {
      "build": "npx prisma generate"
    }
  }
}
